pipeline

{

agent any


stages{

stage('Build'){
steps{
bat "mvn clean"
}
}

stage('Smoke Suite'){
steps{
bat "mvn clean test -Dsurefire.suiteXmlFiles=smoke.xml"
}
}

stage('Sanity Suite'){
steps{
bat "mvn clean test -Dsurefire.suiteXmlFiles=Sanity.xml"
}
}

stage('Regression Suite'){
steps{
bat "mvn clean test -Dsurefire.suiteXmlFiles=fullRegression.xml"
}
}

}

}