pipeline{
      agent any
        stages{
              stage('DockerUp'){
                  steps{
                    bat "mvn clean install -Dsurefire.suiteXmlFiles=src/resources/suites/dockerUp.xml"
                 }
               }  
                stage('Docker Down'){
                  steps{
                    bat "mvn clean install -Dsurefire.suiteXmlFiles=src/resources/suites/dockerTearDown.xml"
                 }
               }  
             }
          }
